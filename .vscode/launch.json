{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Android",
            "type": "reactnative",
            "request": "launch",
            "program": "${workspaceRoot}/.vscode/launchReactNative.js",
            "platform":"android",
            "internalDebuggerPort":9090,
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": true,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/.vscode/.react"
        }
        
    ]
}